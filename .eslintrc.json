{
    "ignorePatterns": ["reportWebVitals.js", "index.js", "setupTests.js", "App.test.js", 
      "dashboard.js", "CovidCard.js","Profile.js", "NewsCard.js", "News.js", "News_Cards.js", "login.js"],
    "extends": "airbnb",
    "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "react/jsx-filename-extension": "off",
        "import/no-named-as-default": "off",
        "import/no-cycle": "off",
        "no-alert": "off",
        "no-console": "off",
        "import/no-named-as-default-member": "off",
        "react/prop-types": "off",
        "dot-notation": "off",
        "prefer-template": "off"
      }
    }
  ]
}